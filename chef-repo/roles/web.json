{
  "name": "web",
  "default_attributes": {
    "php": {
      "configure_options": [
        "--enable-intl",
        "--with-apxs2=/bin/apxs"
      ]
    }
  },
  "override_attributes": {
    "php": {
      "install_method": "source",
      "version": "7.1.3",
      "url": "http://jp2.php.net/get",
      "checksum": "4bfadd0012b966eced448497272150ffeede13136a961aacb9e71553b8e929ec"
    },
    "apache": {
      "version": "2.4",
      "mod_php": {
        "so_filename": "libphp7.so",
        "module_name": "php7"
      }
    }
  },
  "run_list": [
    "recipe[apache2]",
    "recipe[apache2::mod_rewrite]",
    "recipe[playground-web]",
    "recipe[php]",
    "recipe[apache2::mod_php]",
    "recipe[composer]"
  ]
}